{
  "name": "frzrbase",
  "version": "0.0.1",
  "description": "Starter for web projects",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel eleventy:watch esbuild:watch styles:watch",
    "build": "npm-run-all --parallel eleventy:build esbuild:build styles:build",
    "eleventy:watch": "eleventy --serve",
    "eleventy:build": "eleventy",
    "esbuild:watch": "esbuild ./ts/*.ts --outdir=dist/js --watch",
    "esbuild:build": "esbuild ./ts/*.ts --outdir=dist/js",
    "styles:watch": "postcss --watch css/*.sss --dir dist/css --ext css -c postcss.config.js",
    "styles:build": "postcss css/*.sss --dir dist/css --ext css -c postcss.config.js"
  },
  "author": "frzrbox",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.10.0",
    "autoprefixer": "^10.3.4",
    "esbuild": "^0.12.28",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-mixins": "^8.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-simple-vars": "^6.0.3",
    "sugarss": "^4.0.1",
    "typescript": "^4.4.3",
    "yaml": "^1.10.2"
  }
}
